# Production Environment Variables for SkillGenie Frontend
# Copy these to your Vercel environment variables

# API Configuration - Update this with your deployed backend URL
REACT_APP_API_URL=https://your-backend-domain.vercel.app

# Firebase Configuration (if using Firebase on frontend)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_firebase_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id

# Analytics (if using)
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id

# App Configuration
REACT_APP_APP_NAME=SkillGenie
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production
